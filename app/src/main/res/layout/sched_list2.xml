<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".dasboard_activity"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="32dp">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="0dp"
        app:cardCornerRadius="16dp"
        app:cardElevation="4dp"
        app:cardUseCompatPadding="true"
        app:cardBackgroundColor="#FFFFFF"
        tools:layout_editor_absoluteY="32dp">


        <LinearLayout
            android:id="@+id/row2"
            android:layout_width="412dp"
            android:layout_height="174dp"
            android:orientation="horizontal"
            android:padding="10dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            tools:layout_editor_absoluteY="-1dp">

            <androidx.cardview.widget.CardView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_margin="10dp"
                android:layout_weight="1"
                app:cardCornerRadius="20dp"
                app:cardElevation="20dp"
                tools:layout_editor_absoluteX="20dp"
                tools:layout_editor_absoluteY="443dp">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/white"
                    tools:layout_editor_absoluteX="20dp"
                    tools:layout_editor_absoluteY="443dp">

                    <TextView
                        android:id="@+id/textViewDateOff"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentEnd="true"
                        android:layout_marginStart="95dp"
                        android:layout_marginTop="105dp"
                        android:layout_marginEnd="172dp"
                        android:text="Date Off: "
                        android:textColor="#000000"
                        android:textSize="16sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/textViewDateOn"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentEnd="true"
                        android:layout_marginStart="94dp"
                        android:layout_marginTop="78dp"
                        android:layout_marginEnd="176dp"
                        android:layout_marginBottom="8dp"
                        android:text="Date On: "
                        android:textColor="#000000"
                        android:textSize="16sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/textViewTimeOff"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentEnd="true"
                        android:layout_marginStart="94dp"
                        android:layout_marginTop="48dp"
                        android:layout_marginEnd="207dp"
                        android:layout_marginBottom="8dp"
                        android:text="Time Off: "
                        android:textColor="#000000"
                        android:textSize="16sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/textViewTimeOn"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentEnd="true"
                        android:layout_marginStart="92dp"
                        android:layout_marginTop="19dp"
                        android:layout_marginEnd="212dp"
                        android:layout_marginBottom="8dp"
                        android:text="Time On: "
                        android:textColor="#000000"
                        android:textSize="16sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/textViewTimeOn2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentEnd="true"
                        android:layout_marginStart="8dp"
                        android:layout_marginTop="17dp"
                        android:layout_marginEnd="296dp"
                        android:layout_marginBottom="8dp"
                        android:text="Time On: "
                        android:textColor="@color/teal_200"
                        android:textSize="16sp" />

                    <TextView
                        android:id="@+id/textViewTimeOn5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentEnd="true"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="76dp"
                        android:layout_marginEnd="294dp"
                        android:layout_marginBottom="8dp"
                        android:text="Time On: "
                        android:textColor="@color/teal_200"
                        android:textSize="16sp" />

                    <TextView
                        android:id="@+id/textViewTimeOn3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentEnd="true"
                        android:layout_marginStart="9dp"
                        android:layout_marginTop="46dp"
                        android:layout_marginEnd="295dp"
                        android:layout_marginBottom="8dp"
                        android:text="Time On: "
                        android:textColor="@color/teal_200"
                        android:textSize="16sp" />

                    <TextView
                        android:id="@+id/textViewTimeOn4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentEnd="true"
                        android:layout_marginStart="9dp"
                        android:layout_marginTop="105dp"
                        android:layout_marginEnd="295dp"
                        android:layout_marginBottom="8dp"
                        android:text="Time On: "
                        android:textColor="@color/teal_200"
                        android:textSize="16sp" />
                </RelativeLayout>

            </androidx.cardview.widget.CardView>

        </LinearLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/white"
            tools:layout_editor_absoluteX="14dp"
            tools:layout_editor_absoluteY="-20dp">

        </RelativeLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>